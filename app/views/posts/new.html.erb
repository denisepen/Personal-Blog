
<%= form_for @post do |f| %>
  <p class="form-title">Create An Account</p>
  <%= f.label :title %>
<%= f.text_field :title %> <br>
<%# f.label :body %>
<%= f.text_field :body %> <br>

<div class="field">
  <%= f.label :image %>
  <%= f.file_field :image %>

</div>

<div class="field">
  <%= f.label :file %>
  <%= f.file_field :file %>

</div>

<%= f.submit %>
<% end %>
